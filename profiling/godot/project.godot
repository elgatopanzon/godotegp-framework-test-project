; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotEGP Profiling"
run/main_scene="res://scenes/ECSRender.tscn"
run/main_loop_type="GodotEGPMainLoop"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Main="res://addons/godotegp-framework-csharp/classes/Main.cs"

[debug]

settings/stdout/print_fps=true

[display]

window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="GodotEGP Profiling"

[network]

limits/packet_peer_stream/max_buffer_po2=128
limits/debugger/max_chars_per_second=3276800
limits/debugger/max_queued_messages=20480
limits/debugger/max_errors_per_second=4000
limits/debugger/max_warnings_per_second=4000
limits/webrtc/max_channel_in_buffer_kb=6400

[rendering]

renderer/rendering_method.mobile="gl_compatibility"
